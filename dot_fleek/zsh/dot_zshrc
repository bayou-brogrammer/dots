if type brew &>/dev/null; then
    FPATH="$(brew --prefix)/share/zsh/site-functions:${FPATH}"
    autoload -Uz compinit
    compinit
fi
# initialize apps
# direnv
eval "$(direnv hook zsh)"
# atuin
if [[ $options[zle] = on ]]; then
  eval "$(atuin init zsh )"
fi
# starship
eval "$(starship init zsh)"
# zoxide
eval "$(zoxide init zsh)"
